datasource db {
  provider = "postgres"
  url      = "DATABASE_URL"
}

generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
  interface            = "asyncio"
}

// This model describes the Agent Graph/Flow (Multi Agent System).
model Test {
  id   String  @id @default(uuid())
  name String?
}
